<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wms.optimization.mapper.SkillMapper">
    <resultMap id="SkillInfoResultMap" type="com.wms.optimization.entity.SkillInfo">
        <result property="skillId" column="skill_id" />
        <result property="skillName" column="skill_name" />
        <result property="skillLevel" column="skill_level" />
        <result property="productivity" column="productivity" />
        <result property="processSkillSubCategoryCd" column="process_skill_sub_category_cd" />
    </resultMap>

    <select id="selectAllSkills" resultMap="SkillInfoResultMap">
        SELECT skill_id, skill_name, 0 as skill_level, 0 as productivity, process_skill_sub_category_cd FROM skill_master
    </select>
</mapper>
